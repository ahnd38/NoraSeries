<NoraGateway>
	<Gateway callsign="JQ1ZYC G" port="40000">
		<RoutingServices>
			<RoutingService type="JapanTrust">
				<ConfigurationProperties>
					<ServerAddress>trust.d-star.info</ServerAddress>
					<ServerPort>30001</ServerPort>
				</ConfigurationProperties>
			</RoutingService>
		</RoutingServices>
		<Reflectors hostsFile="hosts.txt">
			<Reflector type="DExtra">
				<ConfigurationProperties>
					<MaxReflectors>8</MaxReflectors>
				</ConfigurationProperties>
			</Reflector>
		</Reflectors>
	</Gateway>

	<Repeaters>
